@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<body>
<h2>Stock Prices</h2>
<table>
    <tr>
        <th>Symbol</th>
        <th>Bid</th>
        <th>Ask</th>
    </tr>
    @for (int i = 0; i < Model.Symbols.Count(); i++)
    {
        <tr>
            <td><code>@Model.Symbols[i]</code></td>
            <td><code>@Model.BidPrices[i]</code></td>
            <td><code>@Model.AskPrices[i]</code></td>
        </tr>
    }
</table>
</body>