@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<body>
<h2>Stock Prices</h2>
<table>
    <tr>
        <th>Symbol</th>
        <th>Bid</th>
        <th>Ask</th>
    </tr>
    @for (int i = 0; i < Model.StockPrices.Count(); i++)
    {
        <tr>
            <td>@Model.StockPrices[i].Symbol</td>
            <td>@Model.StockPrices[i].BidPrice</td>
            <td>@Model.StockPrices[i].AskPrice</td>
        </tr>
    }
</table>
</body>